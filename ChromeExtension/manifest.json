{
  "manifest_version":2,
  "name":"Mapper",
  "version":"1.0",
  "background": {
    "scripts": ["libs/servicebusjssdk-1.2.min.js","libs/jquery-3.2.1.min.js","bg.js"]
  },
  "browser_action": {
    "default_icon": "imgs/globe-map-icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["content.js"]
    }
  ]
}